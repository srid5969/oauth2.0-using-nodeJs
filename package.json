{
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.2",
    "oauth2-server": "^3.1.1",
    "uuid": "^9.0.0"
  },
  "name": "nodemailer",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "nodemon src/app.ts",
    "build": "npm test && tsc --build --watch",
    "dev": "ts-node ./src/app.ts",
    "start:dev": "ts-node-dev --watch --respawn --pretty --transpile-only ./src/app.ts",
    "start:prod": "pm2  start ./dist/app.js",
    "start:dev:prod": "nodemon src/app.ts",
    "stop:prod": "pm2  stop ./dist/app.js",
    "restart:prod": "pm2  restart ./dist/app.js --watch",
    "monit:prod": "pm2  monit ./dist/app.js --watch",
    "test": "mocha --require ts-node/register src/test/**/*.ts",
    "jest": "jest"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/oauth2-server": "^3.0.13",
    "babel-jest": "^29.5.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "babel-jest"
    }
  }
}
